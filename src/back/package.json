{
    "name": "sparkplugui-back",
    "version": "1.0.0",
    "license": "GNU",
    "description": "Backend for SparkplugUI",
    "author": "guiklimek",
    "scripts": {
        "compile": "tsc",
        "develop": "nodemon --watch src -e js,ts,json --exec \"ts-node src/index.ts\"",
        "build": "tsup src/index.ts --format cjs --clean",
        "start": "npm run compile && node ./dist/index.js",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@apollo/server": "^4.9.1",
        "@graphql-tools/schema": "^10.0.0",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "graphql": "^16.7.1",
        "graphql-subscriptions": "^2.0.0",
        "graphql-ws": "^5.14.0",
        "mongodb": "^5.7.0",
        "mqtt": "^5.0.5",
        "redis": "^4.6.10",
        "sparkplug-payload": "^1.0.3",
        "ws": "^8.14.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.14",
        "@types/node": "^20.4.8",
        "@types/ws": "^8.5.5",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "tsup": "^7.2.0",
        "typescript": "^5.1.6"
    }
}
